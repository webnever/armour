; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ARMOUR"
run/main_scene="res://game_scenes/mainScene.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/image="res://textures/splash2.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://textures/icon.png"

[autoload]

SceneManager="*res://addons/scene_manager/scene_manager.tscn"
Scenes="*res://addons/scene_manager/scenes.gd"
DamageNumbers="*res://scripts_c#/DamageNumbers.cs"
DialogueManager="*res://scripts_c#/DialogueManager.cs"
GlobalTerminal="*res://scripts_c#/GlobalTerminal.cs"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"
window/vsync/vsync_mode=0
mouse_cursor/custom_image="res://cursor/tex_cursor_pointer.png"

[dotnet]

project/assembly_name="ARMOUR"

[editor]

movie_writer/movie_file="/home/samweaver/Videos/recording.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/gloot/plugin.cfg", "res://addons/scene_manager/plugin.cfg")

[filesystem]

import/fbx/enabled=false

[global_group]

Cameras=""

