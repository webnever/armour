[layer_names]

3d_render/layer_1="backgroundLayer"
3d_render/layer_2="foregroundLayer"
3d_render/layer_3="interactLayer"
3d_physics/layer_2="interactionMask"

[physics]

3d/physics_engine="JoltPhysics3D"
jolt_3d/collisions/areas_detect_static_bodies=true

[rendering]

lights_and_shadows/directional_shadow/soft_shadow_filter_quality=4
lights_and_shadows/positional_shadow/soft_shadow_filter_quality=4
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
anti_aliasing/quality/msaa_2d=2
anti_aliasing/quality/msaa_3d=2
anti_aliasing/quality/use_debanding=true
